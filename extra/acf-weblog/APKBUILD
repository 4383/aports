pkgname=acf-weblog
pkgver=0.1.0
pkgrel=1
pkgdesc="A web-based system administration interface for logfiles"
url="http://git.alpinelinux.org/cgit/acf-weblog"
license="GPL-2"
depends="acf-core lua luasql-postgres wget postgresql-client"
makedepends=""
install=
subpackages=""
source="http://git.alpinelinux.org/cgit/$pkgname/snapshot/$pkgname-$pkgver.tar.bz2"

build() {
	cd "$srcdir"/$pkgname-$pkgver
	make DESTDIR="$pkgdir" install
}

md5sums="3ebba07bab2bb979fcfdb3b3c39a20f3  acf-weblog-0.1.0.tar.bz2"
