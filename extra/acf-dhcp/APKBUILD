pkgname=acf-dhcp
pkgver=0.4.0
pkgrel=0
pkgdesc="A web-based system administration interface for dhcp"
url="http://git.alpinelinux.org/cgit/acf-dhcp"
license="GPL-2"
depends="acf-core lua dhcp"
makedepends=""
