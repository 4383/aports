#!/bin/sh

# we have renamed dhcp to dhcpd. Try cleanup for users

moved=
for i in /etc/runlevel/*/dhcp; do
	if [ -L "$i" ]; then
		mv ${i} ${i}d
		moved=1
	fi
done

if [ -n "$moved" ]; then
	echo " *"
	echo " * NOTICE: the /etc/init.d/dhcp script have been renamed to /etc/init.d/dhcpd"
	echo " *"
fi

