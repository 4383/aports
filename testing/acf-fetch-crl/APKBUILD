pkgname=acf-fetch-crl
pkgver=0.2.0
pkgrel=0
pkgdesc="A web-based system administration interface for fetch-crl"
url="http://git.alpinelinux.org/cgit/$pkgname"
arch="x86 x86_64"
license="GPL-2"
depends="acf-core lua fetch-crl"
