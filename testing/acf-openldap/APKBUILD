pkgname=acf-openldap
pkgver=0.1.1
pkgrel=0
pkgdesc="ACF module for OpenLDAP"
url="http://git.alpinelinux.org/cgit/$pkgname"
arch="noarch"
license="GPL-2"
depends="acf-core lua openldap openldap-back-bdb"
depends_dev=""
makedepends="$depends_dev"
