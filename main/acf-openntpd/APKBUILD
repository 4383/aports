pkgname=acf-openntpd
pkgver=0.6.1
pkgrel=0
pkgdesc="A web-based system administration interface for openntpd"
url="http://git.alpinelinux.org/cgit/acf-openntpd"
arch="noarch"
license="GPL-2"
depends="acf-core lua openntpd"
makedepends=""
