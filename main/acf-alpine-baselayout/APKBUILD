pkgname=acf-alpine-baselayout
pkgver=0.10.0
pkgrel=0
pkgdesc="A web-based system administration interface for alpine-baselayout"
url="http://git.alpinelinux.org/cgit/acf-alpine-baselayout"
arch="noarch"
license="GPL-2"
depends="acf-core lua lua-json4 lua-posix"
