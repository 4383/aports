pkgname=acf-dnsmasq
pkgver=0.3.2
pkgrel=0
pkgdesc="A web-based system administration interface for dnsmasq"
url="http://git.alpinelinux.org/cgit/acf-dnsmasq"
arch="noarch"
license="GPL-2"
depends="acf-core lua dnsmasq"
source="http://dev.alpinelinux.org/archive/$pkgname/$pkgname-$pkgver.tar.bz2"

package() {
	cd "$srcdir/$pkgname-$pkgver"
	make DESTDIR="$pkgdir" install
}
