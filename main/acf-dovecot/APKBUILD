pkgname=acf-dovecot
pkgver=0.2.0
pkgrel=0
pkgdesc="A web-based system administration interface for dovecot"
url="http://git.alpinelinux.org/cgit/acf-dovecot"
license="GPL-2"
depends="acf-core lua dovecot"
makedepends=""
install=
subpackages=""
