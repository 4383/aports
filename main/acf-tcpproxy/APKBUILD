pkgname=acf-tcpproxy
pkgver=0.2.0
pkgrel=0
pkgdesc="A web-based system administration interface for tcpproxy"
url="http://git.alpinelinux.org/cgit/acf-tcpproxy"
arch="x86 x86_64"
license="GPL-2"
depends="acf-core lua tcpproxy"
makedepends=""
install=
subpackages=""
