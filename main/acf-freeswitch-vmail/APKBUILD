pkgname=acf-freeswitch-vmail
pkgver=0.1.0
pkgrel=0
pkgdesc="ACF for a web interface for freeswitch voicemail"
url="http://git.alpinelinux.org/cgit/$pkgname"
arch="noarch"
license="GPL-2"
depends="acf-core lua lua-sql-sqlite3 freeswitch"
makedepends=""
install="$pkgname.post-upgrade"
subpackages=""
source="http://git.alpinelinux.org/cgit/$pkgname/snapshot/$pkgname-$pkgver.tar.bz2"

package() {
	cd "$srcdir"/$pkgname-$pkgver
	make DESTDIR="$pkgdir" install
}

md5sums="07f7a2e1392fc2c85a8922be4a8d6b6e  acf-freeswitch-vmail-0.1.0.tar.bz2"
