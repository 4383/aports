pkgname=acf-freeswitch-vmail
pkgver=0.0.10
pkgrel=0
pkgdesc="ACF for a web interface for freeswitch voicemail"
url="http://git.alpinelinux.org/cgit/$pkgname"
arch="noarch"
license="GPL-2"
depends="acf-core lua lua-sql-sqlite3 freeswitch"
makedepends=""
