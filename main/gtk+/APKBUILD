pkgname=gtk+
pkgver=2.0
pkgrel=0
pkgdesc="metapackage for GTK+ runtime and development"
url=""
arch="all"
license="LGPL"
depends="gtk+2.0"
makedepends="$depends_dev"
install=""
subpackages="$pkgname-dev"
url="http://www.gtk.org/"

dev() {
	depends="gtk+2.0-dev"
	mkdir -p "$subpkgdir"
}

package() {
	mkdir -p "$pkgdir"
}

