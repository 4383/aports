pkgrel=0
pkgdesc="A web-based system administration interface for fetchmail"
url="http://git.alpinelinux.org/cgit/acf-fetchmail"
arch="noarch"
license="GPL-2"
depends="acf-core lua fetchmail"
makedepends=""
