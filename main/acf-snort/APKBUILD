pkgname=acf-snort
pkgver=0.5.0
pkgrel=0
pkgdesc="A web-based system administration interface for snort"
url="http://git.alpinelinux.org/cgit/acf-snort"
license="GPL-2"
depends="acf-core lua snort"
makedepends=""
install=
subpackages=""
