pkgrel=0
pkgdesc="ACF module for mysql"
url="http://git.alpinelinux.org/cgit/$pkgname"
arch="noarch"
license="GPL-2"
depends="acf-core lua mysql mysql-client lua-sql-mysql"
makedepends=""
