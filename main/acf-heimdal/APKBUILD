pkgname=acf-heimdal
pkgver=0.3.0
pkgrel=0
pkgdesc="ACF module for heimdal"
url="http://git.alpinelinux.org/cgit/$pkgname"
arch="noarch"
license="GPL-2"
depends="acf-core lua heimdal"
makedepends=""
