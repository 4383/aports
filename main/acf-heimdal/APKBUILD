subpackages=""
source="http://git.alpinelinux.org/cgit/$pkgname.git/snapshot/$pkgname-$pkgver.tar.bz2"

package() {
        cd "$srcdir/$pkgname-$pkgver"

        make DESTDIR="$pkgdir" install

}

