pkgname=acf-openssh
pkgver=0.5.0
pkgrel=0
pkgdesc="A web-based system administration interface for openssh"
url="http://git.alpinelinux.org/cgit/acf-openssh"
arch="noarch"
license="GPL-2"
depends="acf-core lua openssh"
source="http://dev.alpinelinux.org/archive/$pkgname/$pkgname-$pkgver.tar.bz2"

package() {
	cd "$srcdir/$pkgname-$pkgver"
	make DESTDIR="$pkgdir" install
}
