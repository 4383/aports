pkgname=acf-kamailio
pkgver=0.6.0
pkgrel=0
pkgdesc="A web-based system administration interface for kamailio"
url="http://git.alpinelinux.org/cgit/acf-kamailio"
arch="noarch"
license="GPL-2"
depends="acf-core lua kamailio lua-sql-postgres"
