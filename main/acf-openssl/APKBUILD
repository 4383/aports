pkgname=acf-openssl
pkgver=0.8.0
pkgrel=0
pkgdesc="A web-based system administration interface for openssl"
url="http://git.alpinelinux.org/cgit/acf-openssl"
arch="noarch"
license="GPL-2"
depends="acf-core lua openssl"
makedepends=""
install=
