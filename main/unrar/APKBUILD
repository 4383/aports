# Contributor: Carlo Landmeter <clandmeter@gmail.com>
# Maintainer: Natanael Copa <ncopa@alpinelinux.org>
pkgname=unrar
pkgver=5.0.13
pkgrel=0
pkgdesc="The RAR uncompression program"
url="http://www.rarlab.com"
arch="all"
license="GPL"
depends=
makedepends=
subpackages="$pkgname-doc"
source="http://www.rarlab.com/rar/unrarsrc-$pkgver.tar.gz"

build() {
	cd "$srcdir/$pkgname"
	make -f makefile || return 1
}

package() {
	cd "$srcdir/$pkgname"
	install -Dm755 unrar "$pkgdir"/usr/bin/unrar
	install -Dm644 license.txt "$pkgdir/usr/share/licenses/$pkgname/license.txt"

}

md5sums="901f4957aec5f7ea0a181864a5a3184d  unrarsrc-5.0.13.tar.gz"
sha256sums="71b378c10264b82b8de6f83a8a4e801a98aefd36986425d61657f3fac2c37a09  unrarsrc-5.0.13.tar.gz"
sha512sums="3109adb916d5807407d4dd0e8a289a6258ec30949d6913ebf6ffca7c51a1b6db8682573cce5e03dc2eea154c6a71624c8ad36ff11f34f6dbafa92c63d7b48a4f  unrarsrc-5.0.13.tar.gz"
