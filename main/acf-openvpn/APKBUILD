pkgname=acf-openvpn
pkgver=0.6.0
pkgrel=2
pkgdesc="A web-based system administration interface for openvpn"
url="http://git.alpinelinux.org/cgit/acf-openvpn"
arch="noarch"
license="GPL-2"
depends="acf-core lua openvpn"
makedepends=""
install=
subpackages=""
source="http://dev.alpinelinux.org/archive/$pkgname/$pkgname-$pkgver.tar.bz2"

package() {
	cd "$srcdir/$pkgname-$pkgver"

	make DESTDIR="$pkgdir" install

}

