pkgname=wine_gecko
pkgver=1.5
pkgrel=0
pkgdesc="Wine's built-in replacement for Microsoft's Internet Explorer"
url="http://wiki.winehq.org/Gecko"
arch="x86"
license="MPL"
depends=""
makedepends=""
install=""
subpackages=""
source="http://downloads.sourceforge.net/wine/wine_gecko-$pkgver-$CARCH.msi"

_builddir=
prepare() {
	mkdir -p "$srcdir"
}

build() {
	return 0
}

package() {
	mkdir -p "$pkgdir"/usr/share/wine/gecko/
	cp "$srcdir"/wine_gecko-$pkgver-$CARCH.msi \
		"$pkgdir"/usr/share/wine/gecko/
}

md5sums="ffb862e3e5f66555e058b7a29a1c62ff  wine_gecko-1.5-x86.msi"
